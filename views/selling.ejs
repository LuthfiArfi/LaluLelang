<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Sell</title>
</head>
<body>
  <table>
    <tr>
      <th>No.</th>
      <th>Buyer</th>
      <th>Product</th>
    </tr>
    <% data.Selling.forEach( (a, i) => { %>
      <tr>
        <td><%= i+1%></td>
        <td><%= a.name%></td>
        <td><a href="/product/<%= a.Transaction.ProductId%>"><%= a.Transaction.ProductId%></a></td>
      </tr>
    <% })%>
  </table>
</body>
</html>