<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
  <script src="/assets/jquery.js">
  </script>
    <script src="/assets/jquery.countdown-2.2.0/jquery.countdown.js">
    </script>
    <div class="clock" id="clock">
      <h1></h1>
    </div>
    <script>
    $('#clock').countdown("2018/12/21 04:31", function(event) {
      var totalHours = event.offset.totalDays * 24 + event.offset.hours;
      $(this).html(event.strftime(totalHours + ' hr %M min %S sec'));
    });
    </script>
  </body>
</html>
